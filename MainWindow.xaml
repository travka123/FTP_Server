<Window x:Class="FTP_Server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FTP_Server"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <TextBox x:Name="tbCommand" KeyDown="tbCommand_KeyDown"  DockPanel.Dock="Bottom" Margin="5"/>
        <TextBox x:Name="tbLog" IsReadOnly="True" IsTabStop="False" TextWrapping="Wrap" FontSize="15" FontFamily="Consolas" Margin="5"/>
    </DockPanel>
</Window>
